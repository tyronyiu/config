{
    "languageserver": {
        "graphql": {
            "command": "graphql-lsp",
            "args": ["server", "-m", "stream"],
            // customize filetypes to your needs
            "filetypes": ["typescript", "typescriptreact", "graphql"]
        },
        "sql": {
            "command": "sql-language-server",
            "args" : ["up", "--method", "stdio"],
            "filetypes": ["sql", "mysql"]
        }
    }
}
